{
    "manifest_version": 3,
    "name": "Propeller",
    "version": "1.2",
    "permissions": [
      "activeTab",
      "clipboardRead",
      "clipboardWrite",
      "scripting",
      "storage",
      "commands",
      "contextMenus",
      "tabs"
    ],
    "host_permissions": [
      "https://propel.ziqfm.com/*"
    ],
    "commands": {
      "generate-code": {
        "suggested_key": {
          "default": "Alt+G"
        },
        "description": "Generate Code"
      },
      "copy-selected-text": {
        "suggested_key": {
          "default": "Alt+C"
        },
        "description": "Copy Selected Text"
      },
      "simulate-paste": {
        "suggested_key": {
          "default": "Alt+V"
        },
        "description": "Custom Paste"
      },
      "search-mcq": {
        "suggested_key": {
          "default": "Alt+X"
        },
        "description": "Solve MCQ"
      },
      "search-normal": {
        "description": "Search Normal"
      },
      "image-snip": {
        "description": "Image Snip"
      }
    },
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["contentScript.js"]
      }
    ],
    "icons": {
      "16": "images/icon-16.png",
      "48": "images/icon-48.png",
      "128": "images/icon-128.png"
    }
  }
  
